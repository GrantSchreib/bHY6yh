      <div xml:id="bHY6yh-d15104"><!-- p1l3-r6 -->
        <head xml:id="bHY6yh-d15104-Hd1e108">Rubrica 6</head>
        <head xml:id="bHY6yh-d15104-Hd1e111" type="question-title">De naturali errore circa se et alium</head>
        <p xml:id="bHY6yh-d15104-d1e114">
          <lb ed="#K" n="15"/>de naturali errore circa se et alium 6
        </p>
        <p xml:id="bHY6yh-d15104-d1e119">
          <lb ed="#K" n="16"/> ¶ <ref xml:id="bHY6yh-d15104-Rd1e116">Salomon in prouer capitulo 21</ref> <quote xml:id="bHY6yh-d15104-Qd1e125">Omnis via viri 
          <lb ed="#K" n="17"/>sibi recta videtur</quote>
        </p>
        <p xml:id="bHY6yh-d15104-d1e131">
           <lb ed="#K" n="18"/> ¶<ref xml:id="bHY6yh-d15104-Rd1e130">Aristoteles ethicorum libro 9 capitulo 9</ref> <quote xml:id="bHY6yh-d15104-Qd1e137">Speculari magis possu<lb ed="#K" break="no" n="19"/>mus 
           proximos nos magis quam 
           <lb ed="#K" n="20"/>nobis et illorum actiones quam propria 
           <lb ed="#K" n="21"/>magis</quote> 
        </p>
        <p xml:id="bHY6yh-d15104-d1e144">
           <lb ed="#K" n="22"/> ¶<ref xml:id="bHY6yh-d15104-Rd1e145">Terentius in heutontumerumeno capitulo 9</ref> <quote xml:id="bHY6yh-d15104-Qd1e148">Quod cui<lb ed="#K" break="no" n="23"/>cumque 
           inciderit in mentem volet 
           <lb ed="#K" n="24"/>neque id putabit prauum an rectum 
           <lb ed="#K" n="25"/>sit quod petet ita comparatam esse 
           <lb ed="#K" n="26"/>naturam omnium hominum aliena 
           <lb ed="#K" n="27"/>melius videant et iudicent 
           <lb ed="#K" n="28"/>quam sua an eo sit quod in re nostra 
           <lb ed="#K" n="29"/>sumus gaudio praepediti mimio 
           <lb ed="#K" n="30"/>aut egritudine</quote>
        </p>
        <p xml:id="bHY6yh-d15104-d1e165">
           <lb ed="#K" n="31"/> ¶<ref xml:id="bHY6yh-d15104-Rd1e167">Salustius in catilinario</ref> <quote xml:id="bHY6yh-d15104-Qd1e169">Nulli mortalium
           <lb ed="#K" n="32"/>iniurie sue parue videntur 
           <lb ed="#K" n="33"/>multi eas grauius equo habuere</quote>
        </p>
        <p xml:id="bHY6yh-d15104-d1e177">
              <cb ed="#K" n="b"/>
            <lb ed="#K" n="34"/> ¶<ref xml:id="bHY6yh-d15104-Rd1e181">Tullius tusculanarum quodnum libro i</ref> <quote xml:id="bHY6yh-d15104-Qd1e181" source="http://scta.info/resource/ctd99a-asdvss-d1e374@27-44 http://scta.info/resource/ctd99a-asdvss-d1e400@69-76">Non 
            <lb ed="#K" n="35"/>valet tantum animus vt seipsum 
            <lb ed="#K" n="36"/>videat at vt oculus sic animus 
            <lb ed="#K" n="37"/>se non videns alia cernit men<lb ed="#K" break="no" n="38"/>tis 
            acies seipsam intuens 
            <lb ed="#K" n="39"/>non nunquam ebescit</quote>
        </p>
        <p xml:id="bHY6yh-d15104-d1e193">
            / <ref xml:id="bHY6yh-d15104-Rd1e197">Idem libro 3</ref> 
            <lb ed="#K" n="40"/> <quote xml:id="bHY6yh-d15104-Qd1e195" source="http://scta.info/resource/ctd99a-cuh77a-d1e463@158-164">proprium stultitie aliorum vitia cer<lb ed="#K" break="no" n="41"/>nere 
            obliuisci suorum</quote>
        </p>
        <p xml:id="bHY6yh-d15104-d1e201">
           <lb ed="#K" n="42"/> ¶<ref xml:id="bHY6yh-d15104-Rd1e209">Oratius in sermonibus libro i ser 3</ref> 
           <lb ed="#K" n="43"/> <quote xml:id="bHY6yh-d15104-Qd1e205">Cum tua peruideas oculis mala 
           <lb ed="#K" n="44"/>lippus inunctis cur in amincorum 
           <lb ed="#K" n="45"/>vitiis tam cernis acutum</quote>
        </p>
        <p xml:id="bHY6yh-d15104-d1e213">
           <lb ed="#K" n="46"/> ¶ <ref xml:id="bHY6yh-d15104-Rd1e223">Quintilianus declamationum 4 declam</ref>
           <lb ed="#K" n="47"/> <quote xml:id="bHY6yh-d15104-Qd1e217">humane infirmitatis ista natura est 
           <lb ed="#K" n="48"/>ut ex omnibus accidentibus grauis<lb ed="#K" break="no" n="49"/>simum 
           putet quis que quod patitur</quote> 
        </p>
        <p xml:id="bHY6yh-d15104-d1e238">
          <ref xml:id="bHY6yh-d15104-Rd1e240">Idem 
          <lb ed="#K" n="50"/>5 declam</ref> <quote xml:id="bHY6yh-d15104-Qd1e242">hec omnibus 
          <lb ed="#K" n="51"/>nam est vt sua cuique calami<lb ed="#K" break="no" n="52"/>tas 
          praecipue misera atque to<lb ed="#K" break="no" n="53"/>leranda 
          videatur</quote>
        </p>
        <p xml:id="bHY6yh-d15104-d1e232">
          <lb ed="#K" n="54"/> ¶ <ref xml:id="bHY6yh-d15104-Rd1e254">Seneca epistolarum ad lucillum libro i capitulo i</ref> 
          <lb ed="#K" n="55"/> <quote xml:id="bHY6yh-d15104-Qd1e236">nemo se auarum esse intelligit 
          <lb ed="#K" n="56"/>neque cupidum</quote>
        </p>
        <p xml:id="bHY6yh-d15104-d1e246"> 
          / <ref xml:id="bHY6yh-d15104-Rd1e270">Idem de ira libro 2</ref> 
          <lb ed="#K" n="57"/> <quote xml:id="bHY6yh-d15104-Qd1e250">Aliena vitia ante oculos habe<lb ed="#K" break="no" n="58"/>mus 
          a tergo nostra</quote>
        </p>
        <p xml:id="bHY6yh-d15104-d1e256">
          / <ref xml:id="bHY6yh-d15104-Rd1e280">Idem declama 
          <lb ed="#K" n="59"/>libro 5 capitulo i</ref> <quote xml:id="bHY6yh-d15104-Qd1e258">nemo facile credit 
          <lb ed="#K" n="60"/>quo credito sit dolendum</quote>
        </p>
        <p xml:id="bHY6yh-d15104-d1e266">
           <lb ed="#K" n="61"/>¶ <ref xml:id="bHY6yh-d15104-Rd1e295">Uegecius de re militari libro 3 
           <lb ed="#K" n="62"/>ti 6</ref> <quote xml:id="bHY6yh-d15104-Qd1e270">Interdum imperita rusticit<lb ed="#K" break="no" n="63"/>as 
           plura promittit et credit se 
           <lb ed="#K" n="64"/>scire que nescit</quote>
        </p>
        <p xml:id="bHY6yh-d15104-d1e278">
           <lb ed="#K" n="65"/>¶ <ref xml:id="bHY6yh-d15104-Rd1e309">Lex diges ti qui et a qui manumis 
           <lb ed="#K" n="66"/>et caetera libro i fraudem</ref> <quote xml:id="bHY6yh-d15104-Qd1e282">Sepe de 
           <lb ed="#K" n="67"/>facultatibus suis amplius 
           <pb ed="#K" n="36-v"/>
           <cb ed="#K" n="a"/>
           <lb ed="#K" n="1"/>quam in his est sperant homines</quote>
        </p>
        <p xml:id="bHY6yh-d15104-d1e290">
           <lb ed="#K" n="2"/>¶ <ref xml:id="bHY6yh-d15104-Rd1e325">Marcialis cocus libro vndique 
           <lb ed="#K" n="3"/>suscepto capitulo 5</ref> <quote xml:id="bHY6yh-d15104-Qd1e294">Ad fratris culpam 
           <lb ed="#K" n="4"/>aquile tu lumine portas Ad tua cum 
           <lb ed="#K" n="5"/>scelera nulla vel orba feras</quote>
        </p>
        <p xml:id="bHY6yh-d15104-d1e303">
           <lb ed="#K" n="6"/>¶ <ref xml:id="bHY6yh-d15104-Rd1e339">Cecillius firmianus de beatam vita 
           <lb ed="#K" n="7"/>libro 3</ref>
           <quote xml:id="bHY6yh-d15104-Qd1e307">nemo de se recte iudicare 
           <lb ed="#K" n="8"/>potest</quote>
        </p>
        <p xml:id="bHY6yh-d15104-d1e313">
           <lb ed="#K" n="9"/>¶ <ref xml:id="bHY6yh-d15104-Rd1e351">Prosper sub 93 emp</ref> <quote xml:id="bHY6yh-d15104-Qd1e317">Nemo sue mentis motus non esti<lb ed="#K" break="no" n="10"/>mat 
           equos quodque volunt homines 
           <lb ed="#K" n="11"/>se bene velle putant</quote>
        </p>
        <p xml:id="bHY6yh-d15104-d1e325">
          <lb ed="#K" n="12"/>¶ <ref xml:id="bHY6yh-d15104-Rd1e365">Cassiodorus epistolarum libro i capitulo 4</ref> <quote xml:id="bHY6yh-d15104-Qd1e329">Uolun<lb ed="#K" break="no" n="13"/>tas 
          recta in grauibus passionibus
          <lb ed="#K" n="14"/>non est</quote>
        </p>
        <p xml:id="bHY6yh-d15104-d1e335">
           <lb ed="#K" n="15"/>¶ <ref xml:id="bHY6yh-d15104-Rd1e377">Bernardus libro de improbatione vitiorum capitulo i</ref> <quote xml:id="bHY6yh-d15104-Qd1e339">plures aliena 
           <lb ed="#K" n="16"/>notant crimina nunquam sua</quote>
        </p>
        <p xml:id="bHY6yh-d15104-d1e345">
         Omitted
        </p>
      </div>
    </body>
